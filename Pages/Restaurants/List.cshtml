@page
@model training_ode_to_food.Pages.Restaurants.ListModel
@{
}

<h1>Restaurants</h1>
<h2>@Model.Message</h2>

<table class="table">
    @foreach (var r in Model.Restaurants)
    {
        <tr>
            <td>@r.Id</td>
            <td>@r.Name</td>
            <td>@r.Location</td>
            <td>@r.Cuisine</td>
        </tr>
    }
</table>

